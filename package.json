        "react-icons": "5.2.1"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/node": "22.2.0",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@types/react-icons": "3.0.0",
        "eslint": "8.0.0",
